def factorial(n):

   if not isinstance(n, int):
       raise TypeError("Аргумент должен быть целым числом")
   if n < 0:
       raise ValueError("Факториал отрицательного числа не определён")

   result = 1
   for i in range(1, n + 1):
       result *= i
   return result

def fibonacci(n):

   if not isinstance(n, int):
       raise TypeError("Аргумент должен быть целым числом")
   if n < 0:
       raise ValueError("Количество чисел должно быть неотрицательным")


   if n == 0:
       return []
   elif n == 1:
       return [0]
   elif n == 2:
       return [0, 1]


   fib_list = [0, 1]
   for i in range(2, n):
       fib_list.append(fib_list[i - 1] + fib_list[i - 2])
   return fib_list

def sum_of_digits(number):

   if not isinstance(number, int):
       raise TypeError("Аргумент должен быть целым числом")


   total = 0
   while number > 0:
       total += number % 10
       number //= 10

   return total


import math_utils

print(math_utils.factorial(5))
print(math_utils.fibonacci(8))
print(math_utils.sum_of_digits(85647109))
